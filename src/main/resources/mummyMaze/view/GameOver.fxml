<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" styleClass="background" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mummyMaze.Controller.GameOverController">
   <children>
      <ImageView fitHeight="140.0" fitWidth="359.0" layoutX="321.0" layoutY="72.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/title/gameover.png" />
         </image>
      </ImageView>
      <Label alignment="TOP_CENTER" layoutX="309.0" layoutY="199.0" prefHeight="67.0" prefWidth="385.0" text="YOUR HIGHSCORE" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="34.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Label>
      <TextField fx:id="playerNameInput" alignment="CENTER" layoutX="340.0" layoutY="422.0" prefHeight="50.0" prefWidth="312.0" promptText="Enter Your Name Here" />
      <Label fx:id="scoreLabel" alignment="CENTER" layoutX="419.0" layoutY="291.0" prefHeight="67.0" prefWidth="164.0" textAlignment="CENTER" textFill="WHITE" textOverrun="CLIP">
         <font>
            <Font size="34.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Label>
      <Button defaultButton="true" layoutX="426.0" layoutY="519.0" mnemonicParsing="false" onAction="#gameOverSubmit" prefHeight="63.0" prefWidth="150.0" style="-fx-background-color: #685D12;" text="SUBMIT" textFill="WHITE" />
   </children>
</AnchorPane>
